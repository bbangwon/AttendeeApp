@{
    Layout = "_Layout";

}

@model List<Attendee>

<h1>Attendee</h1>

<ul>
    @foreach (var attendee in Model)
    {
        <li>@attendee.Id - @attendee.UserID - @attendee.Name - @attendee.CreationDate.ToShortDateString()</li>
    }
</ul>