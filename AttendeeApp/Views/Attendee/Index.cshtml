@{
    Layout = "_Layout";

}

@model List<Attendee>

<h1>Attendee</h1>

<a asp-action="Create" class="btn btn-info">참가 등록하기</a>

<ul>
    @foreach (var attendee in Model)
    {
        <li>@attendee.UID - @attendee.UserID - @attendee.Name - @attendee.CreationDate.ToShortDateString()</li>
        
    }
</ul>